* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}

html {
  font-family: sans-serif, serif;
  color: white;
}

.main {
  width: 100vw;
  height: 100%;
  background-color: #570411;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.cardSearch button {
  color: #E6CC80;
  padding: 2px 4px;
  background-color: #570411;
  border: 1px solid white;
}

.cardDisplay {
  border: 2px solid white;
}